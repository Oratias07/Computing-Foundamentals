CHIP HalfSub {
    IN a ,b;
    OUT diff, bOut;

    PARTS:
    Xor(a = a, b = b, out = diff); // compute diff

    // compute bOut
    Not(in = a, out = notA);
    And(a= notA, b = b, out = bOut);
}
