CHIP FullSub {
    IN a,b,bIn;
    OUT diff,bOut;

    PARTS:
    HalfSub(a = a, b = b, diff = diffab, bOut = aHb);
    HalfSub(a = diffab, b = bIn, diff = diff, bOut = diffabHbIn);
    Or(a = aHb, b = diffabHbIn, out = bOut);
}